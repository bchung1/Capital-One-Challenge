{
  "name": "Article Summarizer",
  "version": "1.0",
  "manifest_version": 2,
  "description": "This extension summarizes online articles",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["js/lib/jquery-2.2.1.min.js","js/lib/jquery.highlight.js","js/content.js"]
  }],
  "permissions": [
  "tabs",
  "<all_urls>"
  ]
}


